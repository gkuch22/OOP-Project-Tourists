<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <listener>
    <listener-class>javaFiles.MainContextListener</listener-class>
  </listener>

  <servlet>
    <servlet-name>AnotherUser</servlet-name>
    <servlet-class>javaFiles.AnotherUserServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>SendFriendRequestServlet</servlet-name>
    <servlet-class>javaFiles.SendFriendRequestServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>UnfriendServlet</servlet-name>
    <servlet-class>javaFiles.UnfriendServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>UnfriendServlet</servlet-name>
    <url-pattern>/UnfriendServlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>SendFriendRequestServlet</servlet-name>
    <url-pattern>/SendFriendRequestServlet</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>AnotherUser</servlet-name>
    <url-pattern>/AnotherUser</url-pattern>
  </servlet-mapping>
</web-app>
